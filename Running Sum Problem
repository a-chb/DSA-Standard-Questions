#include <bits/stdc++.h>
using namespace std;


vector<int> runningSum(vector<int>& nums){
    for(int i=1; i<=nums.size();i++)
        nums[i]=nums[i-1]+nums[i];
}


int main(){
    vector<int> v = {1,5,9,3,15,2,7,6};
    for(auto&e:v){
        cout<<e;
    }
    runningSum(v);
    for(int i=0;i<v.size();i++)
        cout<<v[i]<<endl;
    
}
